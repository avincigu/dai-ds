        "urlPath": "/apis/smd/hsm/v1/Subscriptions/SCN",
        "subjects": "stateChange",
        "requestBuilder": "com.intel.dai.builder.ForeignSubscriptionRequest",
        "responseParser": "com.intel.dai.builder.ForeignSubscriptionResponseParser",
        "subscriberName": "daiSubscriptionID",
        "use-ssl": false
      },
      "name": "http_callback"
    },
    "fanTelemetry": {
