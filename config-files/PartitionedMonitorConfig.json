  "providerConfigurations": {
    "com.intel.partitioned_monitor.PartitionedMonitorSystemActions": {
      "sourceType": "rabbitmq",
      "exchangeName": "ucs",
      "uri": "amqp://127.0.0.1"
    },
    "com.intel.dai.monitoring_providers.TelemetryActions": {
      "publishRawTopic": "ucs_raw_data",
      "publishAggregatedTopic": "ucs_aggregated_data",
      "publish": true
    },
    "com.intel.dai.monitoring_providers.BootEventActions": {
      "publishTopic": "ucs_boot_event",
      "doActions": false,
      "bootImageInfoUrlPath": "/bootparameters",
      "publish": true
    },
    "com.intel.dai.monitoring_providers.RasEventTransformer": {
    },
    "com.intel.dai.monitoring_providers.RasEventActions": {
      "publishTopic": "ucs_ras_event",
      "publish": true
    },
    "com.intel.dai.monitoring_providers.TelemetryTransformer": {
      "useAggregation": true,
      "timeWindowSeconds": 600,
      "useTimeWindow": false,
      "windowSize": 10,
      "useMovingAverage": false
    }
  },

  "subjectMap": {
    "telemetry": "EnvironmentalData",
    "inventoryChanges": "InventoryChangeEvent",
    "logs": "LogData",
    "events": "RasEvent",
    "stateChanges": "StateChangeEvent"
  }
